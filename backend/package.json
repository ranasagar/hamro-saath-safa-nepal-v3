{
  "name": "hamro-saath-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "test": "cross-env USE_DB=false tsx node_modules/mocha/bin/mocha tests/**/*.spec.ts --exit",
    "migrate:pg": "tsx src/scripts/runMigrations.ts",
    "coverage": "cross-env USE_DB=false c8 --reporter=lcov --reporter=text tsx node_modules/mocha/bin/mocha tests/**/*.spec.ts --exit"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^8.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "c8": "^7.14.0",
    "chai": "^4.3.7",
    "cross-env": "^10.1.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
